    h1 { font-size: 22pt; color: #00338D; margin-top: 0; margin-bottom: 6pt; font-weight: bold; }
    h2 { font-size: 16pt; color: #00338D; margin-top: 18pt; margin-bottom: 8pt; border-bottom: 2px solid #00338D; padding-bottom: 4pt; }
    h3 { font-size: 13pt; color: #483698; margin-top: 14pt; margin-bottom: 6pt; }
    p { margin: 0 0 8pt 0; }
    table { border-collapse: collapse; width: 100%; margin: 8pt 0 12pt 0; font-size: 10pt; }
