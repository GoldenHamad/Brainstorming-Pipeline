    tr:nth-child(even) td { background-color: #F5F7FA; }
    .step { background-color: #E8F0FE; border-left: 4px solid #00338D; padding: 8pt 12pt; margin: 10pt 0; }
    .step-green { background-color: #E8F5E9; border-left: 4px solid #28A745; padding: 8pt 12pt; margin: 10pt 0; }
    .tip { background-color: #FFF3CD; border-left: 4px solid #FFC107; padding: 8pt 12pt; margin: 10pt 0; }
    ul { margin: 4pt 0 8pt 0; padding-left: 20pt; }
    ol { margin: 4pt 0 8pt 0; padding-left: 20pt; }
